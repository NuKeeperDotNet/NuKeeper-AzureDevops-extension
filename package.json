  "description": "NuKeeper AzureDevOps task",
  "main": "''",
  "scripts": {
    "mv": "@powershell Copy-Item -Force -Recurse .\\node_modules\\* .\\nukeeper\\node_modules\\ ",
    "build": "tsc -p . && tslint -p tsconfig.json",
    "build:prod": "npm i && npm run build && npm run mv",
