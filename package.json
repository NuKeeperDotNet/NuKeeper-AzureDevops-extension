    "tslint": "^5.11.0",
    "typescript": "^3.1.6",
    "typestrict": "^1.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NuKeeperDotNet/NuKeeperAzureDevOps.git"
