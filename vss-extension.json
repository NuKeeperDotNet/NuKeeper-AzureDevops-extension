{
    "manifestVersion": 1,
    "id": "nukeeper",
    "name": "NuKeeper",
    "version": "0.4.33",
    "publisher": "nukeeper",
    "public": true,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Automagically update nuget packages in .NET projects.",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/icon.png"
    },
    "files": [
        {
            "path": "nukeeper"
        }
    ],
    "content": {
        "details": {
            "path": "README.md"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/NuKeeperDotNet/NuKeeper"
    },
    "contributions": [
        {
            "id": "nukeeper",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "nukeeper"
            }
        }
    ]
}